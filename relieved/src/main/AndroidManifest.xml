<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.dialogflowbot">

    <uses-permission android:name="android.permission.INTERNET" />

    <application

        android:allowBackup="true"

        android:label="@string/app_name"


        android:theme="@style/Theme.Relieved">
        <activity android:name="com.example.com.bae.dialogflowbot.Nature_Activity"></activity>
        <activity android:name="com.example.com.bae.dialogflowbot.Alert_Activity" />
        <activity android:name="com.example.com.bae.dialogflowbot.Shinebot_Activity" />
        <activity android:name="com.example.com.bae.dialogflowbot.Sleep_Activity" />
        <activity android:name="com.example.com.bae.dialogflowbot.Food_Activity" />
        <activity android:name="com.example.com.bae.dialogflowbot.Exercise_Activity" />
        <activity android:name="com.example.com.bae.dialogflowbot.Meditation_Activity" />
        <activity
            android:name="com.example.com.bae.dialogflowbot.Home_Activity"
            android:label="Home" />
        <activity
            android:name="com.example.com.bae.dialogflowbot.Splash_Activity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
    </application>

</manifest>